class Solution(object):
    def maxArea(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        n = len(height)
        left = 0 
        right = n -1 
        volume = 0 

        while left < right :
            distance = right - left 
            curr_volume =  distance * min(height[left], height[right])
            volume = max(volume , curr_volume)

            if height[left] < height[right] :
                left += 1 
            else:
                right -= 1 
        
        return(volume)
            
